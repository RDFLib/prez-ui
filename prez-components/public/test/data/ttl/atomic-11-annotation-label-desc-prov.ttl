@prefix ns1: <https://> .
@prefix prez: <https://prez.dev/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ns1:focus-node a sh:focusNode ;
    ns1:pred-with-label-desc-prov ns1:obj-with-label-desc-prov ;
