{
    "compilerOptions": {
      "target": "ES2015",
      "module": "ESNext",
      "declaration": true,
      "declarationDir": "./dist",
      "emitDeclarationOnly": true,
      "sourceMap": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["../../app/lib"],
    "exclude": ["node_modules", "dist"]
}