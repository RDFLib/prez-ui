{
    "name": "prez-components",
    "version": "4.1.4",
    "description": "A Vue.js component library for rendering RDF data for use with Prez UI",
    "type": "module",
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rdflib/prez-ui.git",
        "directory": "packages/prez-components"
    },
    "homepage": "https://github.com/rdflib/prez-ui/tree/main/packages/prez-components#readme",
    "bugs": {
        "url": "https://github.com/rdflib/prez-ui/issues"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/prez-components.umd.cjs",
    "module": "./dist/prez-components.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/prez-components.js",
            "require": "./dist/prez-components.umd.cjs"
        },
        "./prez-components.css": {
            "import": "./dist/prez-components.css",
            "require": "./dist/prez-components.css"
        }
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && vite build",
        "types": "vue-tsc",
        "preview": "vite preview",
        "preinstall": "npx only-allow pnpm"
    },
    "peerDependencies": {
        "vue": "^3.0.0"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.8",
        "@tanstack/vue-table": "^8.21.3",
        "@vueuse/core": "^12.0.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "dompurify": "^3.2.6",
        "lucide-vue-next": "^0.511.0",
        "marked": "^15.0.12",
        "mermaid": "^11.6.0",
        "prez-lib": "workspace:*",
        "reka-ui": "^2.3.0",
        "tailwind-merge": "^3.3.0",
        "tailwindcss": "^4.1.8",
        "tw-animate-css": "^1.3.3",
        "vue-router": "^4.5.1"
    },
    "devDependencies": {
        "@types/node": "^22.15.29",
        "@vitejs/plugin-vue": "^5.2.4",
        "concurrently": "^9.1.2",
        "typescript": "5.8.3",
        "vite": "^6.3.5",
        "vite-plugin-dts": "^4.5.4",
        "vue-tsc": "2.2.10"
    }
}
