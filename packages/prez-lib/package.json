{
    "name": "prez-lib",
    "version": "4.2.0",
    "description": "A JS library for processing RDF data for use with Prez UI",
    "type": "module",
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/RDFLib/prez-ui.git",
        "directory": "packages/prez-lib"
    },
    "homepage": "https://github.com/RDFLib/prez-ui/tree/main/packages/prez-lib#readme",
    "bugs": {
        "url": "https://github.com/RDFLib/prez-ui/issues"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/prez-lib.umd.cjs",
    "module": "./dist/prez-lib.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/prez-lib.js",
            "require": "./dist/prez-lib.umd.cjs"
        }
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "types": "tsc",
        "preview": "vite preview",
        "preinstall": "npx only-allow pnpm"
    },
    "devDependencies": {
        "@rdfjs/types": "^1.1.2",
        "@types/n3": "^1.26.1",
        "typescript": "5.9.2",
        "vite": "^7.3.0",
        "vite-plugin-dts": "^4.5.4"
    },
    "dependencies": {
        "n3": "^1.26.0"
    }
}
